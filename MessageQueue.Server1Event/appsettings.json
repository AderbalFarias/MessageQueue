{
  "ConnectionStrings": {
    "PrimaryConnection": "Data Source=ServerTest;Initial Catalog=DBTest;User Id=test;Password=test;Trusted_Connection=False;MultipleActiveResultSets=true;"
  },
  "AppSettings": {
    "Key1": "Internal Server Error Test"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\MessageQueue\\Logs\\Server1Event\\App.log",
          "fileSizeLimitBytes": "20000",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "20",
          "rollOnFileSizeLimit": "true",
          "shared": "true"
        }
      }
    ]
  },
  "NServiceBusSettings": {
    "ProjectEndpoint": "NsbServer1EventEndpoint",
    "SubscribeToEndpoint": "NsbClientEventEndpoint",
    "RouteToEndpoint": "",
    "NumberOfRetries": 2,
    "PathToLog": "C:\\MessageQueue\\Logs\\Server1Event",
    "SendHeartbeatTo": "NsbServiceControl",
    "AuditProcessedMessagesTo": "NsbAudit",
    "SendFailedMessagesTo": "NsbError",
    "SendMetricDataToServiceControl": "NsbMonitoring",
    "SendMetricDataToServiceControlIntervalInMilliseconds": 500,
    "RecoverabilityTimeIncreaseInSeconds": 5,
    "SubscriptionCacheForInMinutes": 1
  }
}
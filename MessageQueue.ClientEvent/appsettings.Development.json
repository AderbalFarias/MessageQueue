{
  "ConnectionStrings": {
    "dbQueue": "Data Source=localhost;Initial Catalog=DBTest;User Id=sa;Password=afAdmin1;Trusted_Connection=False;MultipleActiveResultSets=true;"
  },
  "AppSettings": {
    "Key1": "Internal Server Error Test"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\MessageQueue\\Logs\\ClientEvent\\App.log",
          "fileSizeLimitBytes": "1000000",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "20",
          "rollOnFileSizeLimit": "true",
          "shared": "true"
        }
      }
    ]
  },
  "NServiceBusSettings": {
    "ProjectEndpoint": "NsbClientEventEndpoint",
    "SubscribeToEndpoint": "",
    "RouteToEndpoint": "",
    "NumberOfImmediateRetries": 2,
    "NumberOfDelayedRetries": 5,
    "PathToLog": "C:\\MessageQueue\\Logs\\ClientEvent",
    "SendHeartbeatTo": "NsbServiceControl",
    "AuditProcessedMessagesTo": "NsbAudit",
    "SendFailedMessagesTo": "NsbError",
    "SendMetricDataToServiceControl": "NsbMonitoring",
    "SendMetricDataToServiceControlIntervalInMilliseconds": 30000,
    "RecoverabilityTimeIncreaseInSeconds": 5,
    "SubscriptionCacheForInMinutes": 1
  }
